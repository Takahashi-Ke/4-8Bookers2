<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>images</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <tbody>
      <tr>
        <td>image</td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
    </tbody>
  <% end %>
</table>